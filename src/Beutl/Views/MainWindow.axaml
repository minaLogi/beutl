<wnd:AppWindow x:Class="Beutl.Views.MainWindow"
               xmlns="https://github.com/avaloniaui"
               xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
               xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
               xmlns:local="using:Beutl.Views"
               xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
               xmlns:ui="using:FluentAvalonia.UI.Controls"
               xmlns:wnd="using:FluentAvalonia.UI.Windowing"
               Title="{Binding WindowTitle.Value}"
               d:DesignHeight="720"
               d:DesignWidth="1280"
               Icon="avares://Beutl.Controls/Assets/logo.png"
               mc:Ignorable="d">
    <local:MainView x:Name="mainView" Background="{DynamicResource MainViewBackground}" />
</wnd:AppWindow>
